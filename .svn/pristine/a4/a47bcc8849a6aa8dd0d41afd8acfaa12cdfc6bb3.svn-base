<body class="notify_bd">
<div class="notify_h20"></div>
<volist name="lists" id="vo">
	<div class="notify">
		<h5>{$vo.title}</h5>
		<b>{$vo.time}</b>
		<a class="pic" href="#"><img src="{$vo.pic}"/></a>
		<p>{$vo.v_title}</p>
		<a class="link" href="{:U('Notice/info',array('id'=>$vo['id']))}">
			<span class="fl">阅读全文</span>
			<em class="fr"><i></i></em>
		</a>
	</div>
</volist>	
