.fangcms_box{
	padding: 20px;
	margin-left: 40px;
}

.fangcms_sms{
	padding: 20px;
}


.lanren{width:220px; overflow:hidden;margin-left: 40px;}
.lanren ul li{
	width:197px;height:98px;
	position:relative;
	overflow:hidden;
	margin-top:10px;
	cursor:pointer;
}
.lanren ul li img{
	width:197px;
	height:98px;
	border:none;
}
.lanren .txt{width:197px;height:25px; background:url(../images/1px.png) repeat;position:absolute;left:0;bottom:0;color:#fff;font-family:"微软雅黑";}
.lanren .txt h3{font-size:20px;font-weight:100;height:45px;text-align:center;line-height:45px;}
.lanren .txt p{font-size:12px;text-align:center;}
